#MCSV-Casino
Variables:
	{slotprefix} = "&6[&eSLOT&6]&r"
	{slot.play.%player%} = false
	{netherslot.play} = false
	{angelslot.play} = false
	{angelslot.pot} = 0
	{bj.%player%} = false

#SpawnPoint=====================================================================================
command /casino:
	trigger:
		teleport player to {casino.spawn}
		send "&6カジノへテレポートしました！"
		stop

command /setcasinospawn:
	permission: admin
	trigger:
		set {casino.spawn} to location of player
		stop

#Debug=====================================================================================
command /casinoreset:
	permission: admin
	trigger:
		send "casino.sk - reset" to player
		loop all players:
			set {jewelry-atari.%loop-player's uuid%} to 0
			set {planks-atari.%loop-player's uuid%} to 0
			set {nether-atari.%loop-player's uuid%} to 0
			set {angel-atari.%loop-player's uuid%} to 0
			set {slot.play.%player%} to false
			set {netherslot.play} to false
			set {angelslot.play} to false
			delete {%event-block%.cards::*}
			delete {%event-block%.player_cards::*}
			delete {%event-block%.dealer_cards::*}
			delete {%event-block%.player}
			set {%event-block%.player.point} to 0
			set {%event-block%.dealer.point} to 0
			set {%event-block%.player_cards} to 0
			set {%event-block%.dealer_cards} to 0
		send "finished" to player

#ブラックジャック=====================================================================================
on sign change:
	if player has permission "op":
		if line 1 of event-block is "[bj]":
			set line 1 of event-block to "&7[&0BLACK&cJACK&7]"
			set line 2 of event-block to "&eD:&d - -"
			set line 3 of event-block to "&eP:&d - -"
			set line 4 of event-block to "&a&l&nクリックで開始！"
			stop

every seconds:
	loop all players:
		if {bj.playtime.%loop-player%} > 0:
			remove 1 from {bj.playtime.%loop-player%}
			if {bj.playtime.%loop-player%} = 0:
				delete {%{bj.table.%loop-player%}%.cards::*}
				delete {%{bj.table.%loop-player%}%.player_cards::*}
				delete {%{bj.table.%loop-player%}%.dealer_cards::*}
				delete {%{bj.table.%loop-player%}%.player}
				set {%{bj.table.%loop-player%}%.player.point} to 0
				set {%{bj.table.%loop-player%}%.dealer.point} to 0
				set {%{bj.table.%loop-player%}%.player_cards} to 0
				set {%{bj.table.%loop-player%}%.dealer_cards} to 0
				set {bj.%loop-player%} to false
				set line 1 of {bj.table.%loop-player%} to "&7[&0BLACK&cJACK&7]"
				set line 2 of {bj.table.%loop-player%} to "&eD:&d - -"
				set line 3 of {bj.table.%loop-player%} to "&eP:&d - -"
				set line 4 of {bj.table.%loop-player%} to "&a&l&nクリックで開始！"
				send "&7[&bB&cJ&7]&cあなたのプレイしていたブラックジャックテーブル台は、1分以上放置されたためリセットされました。" to loop-player

