		set {selectwaterslot.lever} to false
		stop
	
	if {selectwaterslot.potsign} is true:
		set {waterslot.potsign} to event-block
		set {selectwaterslot.potsign} to false
		stop
#看板
on sign change:
	player has permission "admin"
	if line 1 of block is "[water-pot]":	
		set line 1 of block to "&f========================"
		set line 2 of block to "&dPOT: &a%{waterslot.pot}%円"
		set {waterslot.jackpot} to 500000 + {waterslot.pot}
		set line 3 of block to "&d報酬: &a%{waterslot.jackpot}%円"
		set line 4 of block to "&f========================"

#レバーで開始される処理
on right click:
	if event-block is {waterslot.lever}:
		if {waterslot.play} is false:
			if {slot.play.%player%} is false:
				if player's money is less than 10000:
					cancel event
					send "%{slotprefix}% &cスロットを回すには10,000円必要です！"
					stop
				else:
					cancel event
					waterpot()
					set {slot.play.%player%} to true
					set {waterslot.play} to true
					remove 10000 from player's money
					waterslotreel()


