						if {_nitem4} is {_nitem6}:
							add 500000 to player's money
							add 1 to {nether-atari.%player%}
							play sound "entity.player.levelup" to player
							play sound "ui.toast.challenge_complete" to player
							play sound "entity.ender_dragon.ambient" to player
							send title "&0&l&ko &5&lネザースロット &0&l&ko &c&l勝利！" to player
							command "/discordsrv bcast > [SLOT] **__%player%__**さんが、**ネザースロット**で勝利し、**__500,000円__**を受け取りました！"
							broadcast "%{slotprefix}% &b&l%player%&aさんが、&0&l&ko&5&lネザースロット&0&l&ko&aで勝利し、&e&l&n500,000円&aを受け取りました！"
							give enchanted book named "&0&l&ko&5&l悪魔の本&0&l&ko" with lore "&dネザースロットで勝利したもののみに与えられる" and "&d悪魔の邪念が詰まった本。" to player
							set {netherslot.play} to false
							set {slot.play.%player%} to false
							stop
						else:
							cancel event
							play sound "block.note_block.bass" to player
							send "%{slotprefix}% &c残念！外れです！"
							wait 1 second
							set {netherslot.play} to false
							set {slot.play.%player%} to false
							stop
					else:
						cancel event
						play sound "block.note_block.bass" to player
						send "%{slotprefix}% &c残念！外れです！"
						wait 1 second
						set {netherslot.play} to false
						set {slot.play.%player%} to false
						stop
				
			if {slot.play.%player%} is true:
				send "%{slotprefix}% &cスロットプレイ中はほかのスロットを回すことはできません！"
				cancel event
				stop
				
		if {netherslot.play} is true:
			cancel event
			send "%{slotprefix}% &c現在スロットが回っています！"
			stop
