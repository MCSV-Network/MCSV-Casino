Variables:
	{slotprefix} = "&6[&eSLOT&6]&r"
	{slot.play.%player%} = false
	{jewelry-atari.%player%} = 0
	{planks-atari.%player%} = 0
	{nether-atari.%player%} = 0

#宝石スロット=========================================================================================
on sign change:
	player has permission "casino.slots"
	if line 1 of block is "[jewelry-slot]":
		set line 1 of block to "&7-&c宝&6石&eス&aロ&bッ&5ト&7-"
		set line 2 of block to "&e&l&n1回1,000円"
		set line 4 of block to "&a&l&nクリックで回す！"

on right click on sign:
	if line 1 of block is "&7-&c宝&6石&eス&aロ&bッ&5ト&7-":
		if line 2 of block is "&e&l&n1回1,000円":
			if line 4 of block is "&a&l&nクリックで回す！":
				if {slot.play.%player%} is false:
					if player's money is less than 1000:
						send "%{slotprefix}% &cスロットを回すには1,000円必要です！"
						stop
					else:
						send "%{slotprefix}% &e&l&n1,000円&a払いました。"
						set {slot.play.%player%} to true
						remove 1000 from player's money
						set line 4 of block to "&cプレイ中…"
						set {_inv} to chest with 1 row named "&c&l宝&6&l石&e&lス&a&lロ&b&lッ&5&lト"
						open {_inv} to player
						set slot 0 of {_inv} to red stained glass pane named " "
						set slot 1 of {_inv} to orange stained glass pane named " "
						set slot 3 of {_inv} to yellow stained glass pane named " "
						set slot 5 of {_inv} to lime stained glass pane named " "
						set slot 7 of {_inv} to blue stained glass pane named " "
						set slot 8 of {_inv} to magenta stained glass pane named " "
						loop 60 times:
							set {_reel1} to a random integer between 1 and 6
							set {_reel2} to a random integer between 1 and 6
							set {_reel3} to a random integer between 1 and 6
								#リール1の設定
							if {_reel1} is 1:
								set {_item1} to amethyst shard
							if {_reel1} is 2:
								set {_item1} to lapis lazuli
							if {_reel1} is 3:
								set {_item1} to gold ingot
							if {_reel1} is 4:
								set {_item1} to diamond
							if {_reel1} is 5:
								set {_item1} to emerald
							if {_reel1} is 6:
								set {_item1} to netherite ingot
								#リール2の設定
							if {_reel2} is 1:
								set {_item2} to amethyst shard
							if {_reel2} is 2:
								set {_item2} to lapis lazuli
							if {_reel2} is 3:
								set {_item2} to gold ingot
							if {_reel2} is 4:
								set {_item2} to diamond
							if {_reel2} is 5:
								set {_item2} to emerald
							if {_reel2} is 6:
								set {_item2} to netherite ingot

								#リール3の設定
							if {_reel3} is 1:
								set {_item3} to amethyst shard
							if {_reel3} is 2:
								set {_item3} to lapis lazuli
							if {_reel3} is 3:
								set {_item3} to gold ingot
							if {_reel3} is 4:
								set {_item3} to diamond
							if {_reel3} is 5:
								set {_item3} to emerald
							if {_reel3} is 6:
								set {_item3} to netherite ingot

							set slot 2 of {_inv} to {_item1}
							set slot 4 of {_inv} to {_item2}
							set slot 6 of {_inv} to {_item3}
							play sound "block.large_amethyst_bud.place" to player
							wait 1 tick
					set line 4 of block to "&a&l&nクリックで回す！"
					if {_reel1} is {_reel2}:
						if {_reel1} is {_reel3}:
							add 50000 to player's money
							set {slot.play.%player%} to false
							add 1 to {jewelry-atari.%player%}
							play sound "entity.player.levelup" to player
							play sound "ui.toast.challenge_complete" to player
							command "/discordsrv bcast > [SLOT] **__%player%__**さんが、**宝石スロット**で勝利し、**__30,000円__**を受け取りました！"
							broadcast "%{slotprefix}% &b&l%player%&aさんが、&c&l宝&6&l石&e&lス&a&lロ&b&lッ&5&lト&aで勝利し、&e&l&n30,000円&aを受け取りました！"
							wait 1 second
							set {slot.play.%player%} to false
							close player's inventory
							stop

						else:
							play sound "block.note_block.bass" to player
							send "%{slotprefix}% &c残念！外れです！"
							wait 1 second
							set {slot.play.%player%} to false
							close player's inventory
							stop

					else:
						play sound "block.note_block.bass" to player
						send "%{slotprefix}% &c残念！外れです！"
						wait 1 second
						set {slot.play.%player%} to false
						close player's inventory
						stop
				
			if {slot.play.%player%} is true:
				send "%{slotprefix}% &cスロットプレイ中はほかのスロットを回すことはできません！"
				stop
			
		if line 4 of block is "&cプレイ中…":
			send "%{slotprefix}% &c現在スロットが回っています！"
			stop



on inventory click:
	if name of player's current inventory contain "&c&l宝&6&l石&e&lス&a&lロ&b&lッ&5&lト":
		cancel event
